{
  "name": "Unburner",
  "version": "0.3",
  "icons": { "128": "icon.png" },
  "description": "Strip Google Analytics tracker codes.",
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
       ],
      "js": ["unburner.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["tabs"]
}
